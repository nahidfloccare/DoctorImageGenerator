{
    "_meta": {
        "description": "Doctor Image Generator - Flux.1-dev + ReActor Face Swap Workflow",
        "version": "1.0.0",
        "author": "DoctorImageGenerator",
        "notes": "This workflow combines Flux.1-dev for photorealistic generation with ReActor for face swapping"
    },
    "1": {
        "inputs": {
            "ckpt_name": "flux1-dev.safetensors"
        },
        "class_type": "CheckpointLoaderSimple",
        "_meta": {
            "title": "Load Flux.1-dev Checkpoint"
        }
    },
    "2": {
        "inputs": {
            "text": "Professional medical photography, doctor consulting with patient",
            "clip": [
                "1",
                1
            ]
        },
        "class_type": "CLIPTextEncode",
        "_meta": {
            "title": "Positive Prompt"
        }
    },
    "3": {
        "inputs": {
            "text": "low quality, blurry, distorted, deformed, ugly, bad anatomy",
            "clip": [
                "1",
                1
            ]
        },
        "class_type": "CLIPTextEncode",
        "_meta": {
            "title": "Negative Prompt"
        }
    },
    "4": {
        "inputs": {
            "width": 1024,
            "height": 1024,
            "batch_size": 1
        },
        "class_type": "EmptyLatentImage",
        "_meta": {
            "title": "Empty Latent Image"
        }
    },
    "5": {
        "inputs": {
            "seed": 0,
            "steps": 20,
            "cfg": 3.5,
            "sampler_name": "euler",
            "scheduler": "simple",
            "denoise": 1.0,
            "model": [
                "1",
                0
            ],
            "positive": [
                "2",
                0
            ],
            "negative": [
                "3",
                0
            ],
            "latent_image": [
                "4",
                0
            ]
        },
        "class_type": "KSampler",
        "_meta": {
            "title": "KSampler (Flux)"
        }
    },
    "6": {
        "inputs": {
            "samples": [
                "5",
                0
            ],
            "vae": [
                "1",
                2
            ]
        },
        "class_type": "VAEDecode",
        "_meta": {
            "title": "VAE Decode"
        }
    },
    "7": {
        "inputs": {
            "image": "doctor_face.jpg",
            "upload": "image"
        },
        "class_type": "LoadImage",
        "_meta": {
            "title": "Load Doctor Face"
        }
    },
    "8": {
        "inputs": {
            "enabled": true,
            "swap_model": "inswapper_128.onnx",
            "facedetection": "retinaface_resnet50",
            "face_restore_model": "GFPGANv1.4.pth",
            "face_restore_visibility": 1.0,
            "codeformer_weight": 0.5,
            "detect_gender_input": "no",
            "detect_gender_source": "no",
            "input_faces_index": "0",
            "source_faces_index": "0",
            "console_log_level": 1,
            "input_image": [
                "6",
                0
            ],
            "source_image": [
                "7",
                0
            ]
        },
        "class_type": "ReActorFaceSwap",
        "_meta": {
            "title": "ReActor Face Swap"
        }
    },
    "9": {
        "inputs": {
            "upscale_model_name": "4x-UltraSharp.pth",
            "image": [
                "8",
                0
            ]
        },
        "class_type": "ImageUpscaleWithModel",
        "_meta": {
            "title": "Upscale 4x"
        }
    },
    "10": {
        "inputs": {
            "width": 2048,
            "height": 2048,
            "interpolation": "lanczos",
            "method": "keep proportion",
            "condition": "always",
            "multiple_of": 0,
            "image": [
                "9",
                0
            ]
        },
        "class_type": "ImageResize+",
        "_meta": {
            "title": "Resize to Final"
        }
    },
    "11": {
        "inputs": {
            "filename_prefix": "doctor_generated",
            "images": [
                "10",
                0
            ]
        },
        "class_type": "SaveImage",
        "_meta": {
            "title": "Save Image"
        }
    }
}